    <insert id="syncProbeDiffToBackup">
        INSERT INTO metoo_probe_bck
        SELECT
        *
        FROM
        metoo_probe p
        WHERE
        NOT EXISTS (
        SELECT 1
        FROM metoo_probe_bck b
        WHERE b.mac = p.mac
        AND b.port_num = p.port_num
        );
    </insert>
