            </if>
            <if test="port_num != null and port_num != ''">
                AND port_num = #{port_num}
            </if>
            <if test="ttlGt != null and ttlGt != ''">
                AND ttl &gt; #{ttlGt}
            </if>
            <if test="IpvIsNotNull != null and IpvIsNotNull.trim() != ''">
