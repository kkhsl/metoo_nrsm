<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.metoo.nrsm.core.mapper.Dhcp6Mapper">

    <select id="selectObjById" parameterType="java.lang.Long" resultType="com.metoo.nrsm.entity.Dhcp6">
        SELECT *
        FROM metoo_dhcp6
        WHERE
          id = #{id}
    </select>

    <select id="selectObjByLease" parameterType="java.lang.String" resultType="com.metoo.nrsm.entity.Dhcp6">
        SELECT *
        FROM metoo_dhcp6
        WHERE
          lease = #{lease}
    </select>

    <select id="selectConditionQuery" parameterType="com.metoo.nrsm.core.dto.Dhcp6Dto" resultType="com.metoo.nrsm.entity.Dhcp6">
        SELECT *
        FROM metoo_dhcp6
        <where>
            <if test="ia_na != '' and ia_na != null">
                AND ia_na = #{ia_na}
            </if>
        </where>
    </select>

    <select id="selectObjByMap" parameterType="java.util.Map" resultType="com.metoo.nrsm.entity.Dhcp6">
        SELECT *
        FROM metoo_dhcp6
        <where>
            <if test="ia_na != '' and ia_na != null">
                AND ia_na = #{ia_na}
            </if>
            <if test="iaaddr_array != '' and iaaddr_array != null">
                AND iaaddr in
                <foreach collection="iaaddr_array" index = "index" item="item" separator="," open="(" close=")">
                    #{item}
                </foreach>
            </if>
        </where>
    </select>


    <insert id="save" parameterType="com.metoo.nrsm.entity.Dhcp6">
        INSERT INTO metoo_dhcp6
        <trim prefix="(" suffixOverrides="," suffix=")">
            <if test="addTime != null">
                addTime,
            </if>
            <if test="ia_na != '' and ia_na != null">
                ia_na,
            </if>
            <if test="cltt != '' and cltt != null">
                cltt,
            </if>
            <if test="iaaddr != '' and iaaddr != null">
                iaaddr,
            </if>
            <if test="binding_state != '' and binding_state != null">
                binding_state,
            </if>
            <if test="preferred_life != '' and preferred_life != null">
                preferred_life,
            </if>
            <if test="max_life != '' and max_life != null">
                max_life,
            </if>
            <if test="ends != '' and ends != null">
                ends,
            </if>
        </trim>
        <trim prefix="VALUES(" suffixOverrides="," suffix=")">
            <if test="addTime != null">
                #{addTime},
            </if>
            <if test="ia_na != '' and ia_na != null">
                #{ia_na},
            </if>
            <if test="cltt != '' and cltt != null">
                #{cltt},
            </if>
            <if test="iaaddr != '' and iaaddr != null">
                #{iaaddr},
            </if>
            <if test="binding_state != '' and binding_state != null">
                #{binding_state},
            </if>
            <if test="preferred_life != '' and preferred_life != null">
                #{preferred_life},
            </if>
            <if test="max_life != '' and max_life != null">
                #{max_life},
            </if>
            <if test="ends != '' and ends != null">
                #{ends},
            </if>
        </trim>
    </insert>

    <update id="update" parameterType="com.metoo.nrsm.entity.Dhcp6">
        UPDATE FROM metoo_dhcp6
        <set>
            <if test="addTime != null">
                addTime = #{addTime},
            </if>
            <if test="ia_na != '' and ia_na != null">
                ia_na = #{ia_na},
            </if>
            <if test="cltt != '' and cltt != null">
                cltt = #{cltt},
            </if>
            <if test="iaaddr != '' and iaaddr != null">
                iaaddr = #{iaaddr},
            </if>
            <if test="binding_state != '' and binding_state != null">
                binding_state = #{binding_state},
            </if>
            <if test="preferred_life != '' and preferred_life != null">
                preferred_life = #{preferred_life},
            </if>
            <if test="max_life != '' and max_life != null">
                max_life = #{max_life},
            </if>
            <if test="ends != '' and ends != null">
                ends = #{ends},
            </if>
        </set>
        where id = #{id}
    </update>

    <delete id="delete" parameterType="java.lang.Long">
        DELETE FROM metoo_dhcp6 where id = #{id}
    </delete>

    <update id="truncateTable">
        TRUNCATE TABLE metoo_dhcp6
    </update>

    <delete id="deleteTable">
        DELETE FROM metoo_dhcp6
    </delete>

</mapper>