ALTER TABLE `metoo_flux_statistics`
MODIFY COLUMN `ipv4` DECIMAL(30,5) DEFAULT NULL ,
MODIFY COLUMN `ipv6` DECIMAL(30,5) DEFAULT NULL,
MODIFY COLUMN `ipv6Rate` DECIMAL(22,2) DEFAULT NULL;

ALTER TABLE `metoo_flux_statistics`
MODIFY COLUMN `ipv4Sum` DECIMAL(30,10) DEFAULT NULL,
MODIFY COLUMN `ipv6Sum` DECIMAL(30,10) DEFAULT NULL;

CREATE TABLE `metoo_flux` (
  `id` bigint NOT NULL AUTO_INCREMENT,
  `addTime` datetime DEFAULT NULL,
  `deleteStatus` int DEFAULT '0' COMMENT '软删除',
  `ipv4` decimal(20,2) DEFAULT NULL,
  `ipv6` decimal(20,2) DEFAULT NULL,
  `ipv6Rate` decimal(20,2) DEFAULT NULL,
  `ipv4Sum` decimal(20,0) DEFAULT NULL,
  `ipv6Sum` decimal(20,0) DEFAULT NULL,
  PRIMARY KEY (`id`) USING BTREE,
  KEY `index_addTime` (`addTime`)
) ENGINE=InnoDB AUTO_INCREMENT=41 DEFAULT CHARSET=utf8mb3 ROW_FORMAT=DYNAMIC;